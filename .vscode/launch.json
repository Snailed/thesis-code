{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "${command:pickArgs}"
            ]
        },
        {
            "name": "Dry Run",
            "type": "debugpy",
            "request": "launch",
            "program": "code/main.py",
            "console": "integratedTerminal",
            "args": [
                "sample",
                "--dataset",
                "boston-housing",
                "concrete",
                "energy",
                "--model",
                "UCI_BNN_2_50",
                "--n_samples",
                "3000",
                "--n_warmup",
                "500",
                "--seed",
                "10"
            ]
        },
        {
            "name": "MAP",
            "type": "debugpy",
            "request": "launch",
            "program": "code/main.py",
            "console": "integratedTerminal",
            "args": [
                "map",
                "--dataset",
                "boston-housing",
                "--model",
                "UCI_BNN_2_50",
                "--n_steps",
                "10000",
                "--seed",
                "42"
            ]
        }
    ]
}